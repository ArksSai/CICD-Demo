pipeline {
    agent any
    stages {
        stage("start") {
            steps {
                echo "start"
                script {
                    sh """
                    echo start
                    """
                }
            }
        }
        stage("dev") {
            steps {
                echo "dev"
                script {
                    sh """
                    echo dev
                    """
                }
            }
        }
        stage("test") {
            steps {
                echo "test"
                script {
                    sh """
                    echo test
                    """
                }
            }
        }
        stage("deploy") {
            steps {
                echo "deploy"
                script {
                    sh """
                    echo deploy
                    """
                }
            }
        }
    }
}
